@model GetTaskCustomerListResult
@{
    ViewData["Title"] = "Task Customer List";
}

<div class="container">
    <div class="row">

        <div class="col-md-12">
            <div class="table-responsive">
                <table id="mytable" class="table table-bordred table-striped">

                    <thead>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>TaskId</th>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.TaskCustomerListResultItems)
                        {
                            <tr>
                                <td>@item.FirstName</td>
                                <td>@item.LastName</td>
                                <td>@item.TaskId</td>
                            </tr>
                        }
                    </tbody>

                </table>

            </div>

        </div>
    </div>
</div>